<h1>You are in report view page</h1>




<table>
  <thead>
    <tr>
      <th>Txn ID</th>
      <th>    </th>
      <th>Amount</th>
      <th>    </th>
      <th>Product Name</th>
      <th>     </th>
      <th>User Name</th>
      <th>     </th>
      <th>Transaction Date</th>
      <th colspan="6"></th>
    </tr>
  </thead>

  <tbody>
    <% @all_txn.each do |transaction| %>
      <tr>
        <td><%= transaction.id %></td>
        <td>            </td>
        <td><%= transaction.amount %></td>
        <td>   </td>
        <td><%= transaction.product.name %></td>
        <td>   </td>
        <td><%= transaction.user.name %></td>
        <td>   </td>
        <td><%= transaction.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>